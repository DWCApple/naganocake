<table>
    <thead>
    <h3>ジャンル編集</h3>
    </thead>
    <%= form_with model: @genre, url: admin_genre_path(@genre.id) do |f| %>
    <tbody>
      <tr>
        <td>ジャンル名</td>
        <td><%= f.text_field :name %></td>
        <td><%= f.submit '変更を保存' %></td>
      </tr>
    </tbody>
    <% end %>
</table>
