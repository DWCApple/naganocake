<h1>会員一覧</h1>
<table class="table"> 
  <thead>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
    <tbody>
      <% @end_users.each do |end_user| %>
      <tr>
        <td><%= end_user.id %></td>
        <td>
          <%= link_to admin_end_user_path(end_user.id) do %>
          <%= end_user.family_name %><%= end_user.first_name %>
          <% end %>  
        </td>
        <td><%= end_user.email %></td>
        <td>
          <% if end_user.is_deleted == false %>
            <%= "有効" %>
          <% else %>
            <%= "退会" %>
          <% end %>
          </td>
      </tr>
      <% end %>
    </tbody>
</table>
<%= paginate @end_users %>
